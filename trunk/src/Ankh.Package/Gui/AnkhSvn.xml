      <Button id="Cmd_Log" name="Log" priority="0x1800"
								text="View &amp;History"
								toolTip="Show the change log for the selected item." iconFile="PendingChanges/ViewHistory.png" defaultInvisible="true"/>      
      <Menu id="Menu_ItemResolve" priority="0x1A00" name="Resolve" text="Resol&amp;ve">
        <Group id="Grp_ItemResolveCommand" priority="0x100">
          <Button id="Cmd_ItemResolveMerge" priority="0x10" text="Resolve &amp;Conflict" defaultInvisible="true" />
          <Button id="Cmd_ItemResolveMergeTool" priority="0x20" text="Resolve using &amp;Tool" defaultInvisible="true" />
        </Group>
        <Group id="Grp_ItemResolveFile" priority="0x200">
          <Button id="Cmd_ItemResolveWorking" priority="0x10" text="Using &amp;Working copy file" defaultInvisible="true" />
