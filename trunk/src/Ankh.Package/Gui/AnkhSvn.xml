<?xml version="1.0" encoding="utf-8" ?>
<VsGui version="1.0"  xmlns="http://schemas.studioturtle.net/2007/01/gui/"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://schemas.studioturtle.net/2007/01/gui/ Gui.xsd">
  <Imports>
    <Import include="stdidcmd.h" />
    <Import include="vsshlids.h" />
    <Import include="msobtnid.h" />
    <Import type="AnkhSvn.Ids.AnkhId" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Ankh_" />
    <Import type="AnkhSvn.Ids.AnkhCommand" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Cmd_" />
    <Import type="AnkhSvn.Ids.AnkhCommandGroup" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Grp_" />
    <Import type="AnkhSvn.Ids.AnkhCommandMenu" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Menu_" />
  </Imports>
  <Defines>
  </Defines>
  <UI packageId="Ankh_PackageGuid" autoBmpId="Ankh_BmpGuid">
    <MenuRef id="guidSHLMainMenu:IDM_VS_MENU_FILE">
      <!-- Visual Studio file menu -->
      <Group id="Grp_FileMenuScc" priority="0x0800">
        <Menu id="Menu_FileScc" priority="0x0100" name="SvnMenu" text="Sub&amp;version">
          <Group id="Grp_FileSccMenuTasks" priority="0x0100">
            <Button id="Cmd_Checkout" name="Checkout" priority="0x301"
                    text="Chec&amp;kout a Repository..."
                    toolTip="Checkout a Subversion repository." iconFile="menu/CheckoutDirectory.png"/>            
            <Button id="Cmd_AddSolutionToRepository" name="AddSolutionToRepository" priority="0x301"
                  text="Add Sol&amp;ution to Subversion repository..."
                  toolTip="Add this solution to a Subversion repository." iconFile="menu/AddSolutionToRepository.png" />
            
            <Button id="Cmd_AddItem" name="AddItem" priority="0x301"
                    text="&amp;Add..."
                    toolTip="Add selected items to the working copy." iconFile="menu/Add.png" defaultInvisible="true"/>
            <Button id="Cmd_Blame" name="Blame" priority="0x301"
                    text="&amp;Blame..."
                    toolTip="Identify which user wrote which lines." iconFile="menu/Blame.png" defaultInvisible="true"/>            
            <Button id="Cmd_CheckoutSolution" name="CheckoutSolution" priority="0x301"
                    text="Checkout &amp;Solution..."
                    toolTip="Checkout this solution." iconFile="menu/CheckoutSolution.png" defaultInvisible="true"/>
            <Button id="Cmd_Cleanup" name="Cleanup" priority="0x301"
                    text="Cl&amp;eanup"
                    toolTip="Cleanup the working copy." iconFile="menu/Cleanup.png" defaultInvisible="true"/>
            <Button id="Cmd_CommitItem" name="CommitItem" priority="0x301"
                    text="&amp;Commit..."
                    toolTip="Commit selected items to the Subversion repository." iconFile="menu/Commit.png" defaultDisabled="true"/>
            <Button id="Cmd_CreatePatch" name="CreatePatch" priority="0x301"
                    text="Create &amp;Patch..."
                    toolTip="Create a patch file of changes." iconFile="menu/CreatePatch.png" defaultInvisible="true"/>
            <Button id="Cmd_DiffExternalLocalItem" name="DiffExternalLocalItem" priority="0x301"
                    text="Diff E&amp;xternal"
                    toolTip="Diff against local text base using external tool." iconFile="menu/Diff.png" defaultInvisible="true"/>
            <Button id="Cmd_DiffLocalItem" name="DiffLocalItem" priority="0x301"
                    text="Di&amp;ff..."
                    toolTip="Show differences compared to local text base." iconFile="menu/Diff.png" defaultInvisible="true"/>
            <Button id="Cmd_Export" name="Export" priority="0x301"
                    text="E&amp;xport a Repository or Local Folder..."
                    toolTip="Export a Subversion repository or local folder." iconFile="menu/Export.png" defaultInvisible="true"/>
            <Button id="Cmd_ExportFolder" name="ExportFolder" priority="0x301"
                    text="E&amp;xport this folder"
                    toolTip="Export this folder." iconFile="menu/Export.png" defaultInvisible="true"/>
            <Button id="Cmd_Lock" name="Lock" priority="0x301"
                    text="Loc&amp;k..."
                    toolTip="Lock the selected item." iconFile="menu/Lock.png" />
            <Button id="Cmd_Log" name="Log" priority="0x301"
                    text="&amp;Log"
                    toolTip="Show the change log for the selected item." iconFile="menu/Log.png" />
            <Button id="Cmd_Refresh" name="Refresh" priority="0x301"
                    text="Refres&amp;h"
                    toolTip="Refresh this view." iconFile="menu/Refresh.png"/>
            <Button id="Cmd_Relocate" name="Relocate" priority="0x301"
                    text="Relo&amp;cate..."
                    toolTip="Relocate this file." iconFile="menu/Relocate.png" />
            <Button id="Cmd_RemoveRepositoryRoot" name="RemoveRepositoryRoot" priority="0x301"
                    text="&amp;Remove Repository URL"
                    toolTip="Remove a URL from the Repository Explorer." iconFile="menu/RemoveUrl.png"/>
            <Button id="Cmd_ResolveConflict" name="ResolveConflict" priority="0x301"
                    text="Res&amp;olve Conflict..."
                    toolTip="Resolve conflict between changes." iconFile="menu/ResolveConflict.png"/>
            <Button id="Cmd_ResolveConflictExternal" name="ResolveConflictExternal" priority="0x301"
                    text="Resolve Conflict E&amp;xternal..."
                    toolTip="Resolve conflict between changes using external tool." iconFile="menu/ResolveConflict.png"/>
            <Button id="Cmd_RevertItem" name="RevertItem" priority="0x301"
                    text="&amp;Revert"
                    toolTip="Revert this item to last updated revision." iconFile="menu/Revert.png"/>
            <Button id="Cmd_RevertToRevision" name="RevertToRevision" priority="0x301"
                    text="Re&amp;vert to Revision..."
                    toolTip="Revert this item to a specific revision." iconFile="menu/RevertToVersion.png" />
            <Button id="Cmd_SaveToFile" name="SaveToFile" priority="0x301"
                    text="Save to &amp;File"
                    toolTip="Save the file to disk." iconFile="menu/SaveToFile.png" />
            <Button id="Cmd_SwitchItem" name="SwitchItem" priority="0x301"
                    text="&amp;Switch..."
                    toolTip="Switch this item to a different URL." iconFile="menu/Switch.png"/>
            <Button id="Cmd_Unlock" name="Unlock" priority="0x301"
                    text="U&amp;nlock..."
                    toolTip="Unlock the selected items." iconFile="menu/Unlock.png"/>
            <Button id="Cmd_UpdateItem" name="UpdateItem" priority="0x301"
                    text="&amp;Update..."
                    toolTip="Updates local items with changes from the Subversion repository." iconFile="menu/Update.png"/>
          </Group>
          <Group id="Grp_FileSccMenuManagement" priority="0x0200">
            <Button id="Cmd_ShowCommitDialog" name="ShowCommitDialog" priority="0x301"
                    text="Show &amp;Commit Dialog"
                    toolTip="Show the commit dialog." iconFile="menu/Commit.png"/>
            <Button id="Cmd_EditConfigFile" name="EditConfigFile" priority="0x301"
                    text="&amp;Configuration..."
                    toolTip="Configure AnkhSVN." iconFile="menu/EditConfigFile.png" />
            <Button id="Cmd_SendFeedback" name="SendFeedback" priority="0x301"
                    text="Send Feedback..."
                    toolTip="Send the AnkhSVN team comments and suggestions." iconFile="menu/SendSuggest.png" />
            <Button id="Cmd_ToggleAnkh" name="ToggleAnkh" priority="0x301"
                    text="Enable Ank&amp;hSVN"
                    toolTip="Enable Subversion management on this project." iconFile="menu/ToggleAnkh.png" textChanges="true"/>
            <Button id="Cmd_RunSvnCommand" name="RunSvn" text="RunSvn" priority="0xffff" commandWellOnly="true" allowParams="true" />
            <Button id="Cmd_MarkProjectDirty" name="MarkProjectDirty" text="MarkProjectDirty" priority="0xffff" commandWellOnly="true" allowParams="true" />
          </Group>
        </Menu>
      </Group>
    </MenuRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_VIEW_ORG_WINDOWS">
      <!-- View - The Group containing the Solution Explorer,.. -->
      <Button id="Cmd_ShowRepositoryExplorer" name="RepositoryExplorer" priority="0x901"
                  text="Repositor&amp;y Explorer"
                  toolTip="Show the Repository Explorer window." iconFile="menu/ReposExplorer.png">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="r"/>
      </Button>
      <Button id="Cmd_ShowWorkingCopyExplorer" name="WorkingCopyExplorer" priority="0x902"
              text="&amp;Working Copy Explorer"
              toolTip="Show the Working Copy Explorer window." iconFile="menu/WorkExplorer.png">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="w"/>
      </Button>
    </GroupRef>

    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_SOLUTION_SCC">
      <!-- VS Predefined group for SCC commands on the solution -->
      <ButtonRef id="Cmd_UpdateItem" priority="0x10" />
      <ButtonRef id="Cmd_CommitItem" priority="0x20" />
      <ButtonRef id="Cmd_DiffLocalItem" priority="0x30" />
      <ButtonRef id="Cmd_RevertItem" priority="0x40" />
      <Menu id="Menu_SolutionExplorerSccForSolution" priority="0x50" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForSolution" priority="0x100">
          <ButtonRef id="Cmd_AddItem" priority="0x10" />
          <ButtonRef id="Cmd_SwitchItem" priority="0x20" />
          <ButtonRef id="Cmd_Log" priority="0x30" />
          <ButtonRef id="Cmd_Lock" priority="0x40" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x90" />
          <ButtonRef id="Cmd_ResolveConflict" priority="0xA0" />
          <ButtonRef id="Cmd_ResolveConflictExternal" priority="0xB0" />
          <ButtonRef id="Cmd_RevertItem" priority="0xC0" />
          <ButtonRef id="Cmd_RevertToRevision" priority="0xD0" />
          <ButtonRef id="Cmd_Refresh" priority="0xE0" />
          <ButtonRef id="Cmd_ToggleAnkh" priority="0xF0" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_PROJECT_SCC">
      <!-- VS Predefined group for SCC commands on projects -->
      <ButtonRef id="Cmd_UpdateItem" priority="0x10" />
      <ButtonRef id="Cmd_CommitItem" priority="0x20" />
      <ButtonRef id="Cmd_DiffLocalItem" priority="0x30" />
      <ButtonRef id="Cmd_RevertItem" priority="0x40" />
      <Menu id="Menu_SolutionExplorerSccForProject" priority="0x50" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForProject" priority="0x100">
          <ButtonRef id="Cmd_AddItem" priority="0x10" />
          <ButtonRef id="Cmd_SwitchItem" priority="0x20" />
          <ButtonRef id="Cmd_Log" priority="0x30" />
          <ButtonRef id="Cmd_Lock" priority="0x40" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x90" />
          <ButtonRef id="Cmd_ResolveConflict" priority="0xA0" />
          <ButtonRef id="Cmd_ResolveConflictExternal" priority="0xB0" />
          <ButtonRef id="Cmd_RevertItem" priority="0xC0" />
          <ButtonRef id="Cmd_RevertToRevision" priority="0xD0" />
          <ButtonRef id="Cmd_Refresh" priority="0xE0" />
          <ButtonRef id="Cmd_ToggleAnkh" priority="0xF0" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_ITEM_SCC">
      <!-- VS Predefined group for SCC commands on project items -->
      <ButtonRef id="Cmd_UpdateItem" priority="0x10" />
      <ButtonRef id="Cmd_CommitItem" priority="0x20" />
      <ButtonRef id="Cmd_DiffLocalItem" priority="0x30" />
      <ButtonRef id="Cmd_RevertItem" priority="0x40" />
      <Menu id="Menu_SolutionExplorerSccForItem" priority="0x50" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForItem" priority="0x100">
          <ButtonRef id="Cmd_AddItem" priority="0x10" />
          <ButtonRef id="Cmd_SwitchItem" priority="0x20" />
          <ButtonRef id="Cmd_Log" priority="0x30" />
          <ButtonRef id="Cmd_Lock" priority="0x40" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x90" />
          <ButtonRef id="Cmd_ResolveConflict" priority="0xA0" />
          <ButtonRef id="Cmd_ResolveConflictExternal" priority="0xB0" />
          <ButtonRef id="Cmd_RevertItem" priority="0xC0" />
          <ButtonRef id="Cmd_RevertToRevision" priority="0xD0" />
          <ButtonRef id="Cmd_Refresh" priority="0xE0" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_RepositoryExplorerToolBar">
      <!-- The Repository Explorer Toolbar -->
      <Menu id="Menu_RepositoryExplorerToolBar" name="RepositoryExplorer" priority="0" type="ToolWindowToolbar">

        <Group id="Grp_RepositoryExplorerToolBar" priority="0x100">
          <Button id="Cmd_AddRepositoryRoot" name="AddRepositoryRoot" priority="0x301"
                    text="A&amp;dd Repository Url"
                    toolTip="Add a new URL to the Repository Explorer." iconFile="menu/AddUrl.png" />
          <Button id="Cmd_CopyReposExplorerUrl" name="CopyReposExplorerUrl" priority="0x301"
                    text="Copy &amp;Url"
                    toolTip="Copy the Url of this item to the clipboard." iconFile="menu/CopyUrlToClipboard.png" />
          <Button id="Cmd_NewDirectory" name="NewDirectory" priority="0x301"
                    text="Ne&amp;w Directory..."
                    toolTip="Create a new directory here." iconFile="menu/MakeDirectory.png" />
          <Button id="Cmd_RefreshRepositoryItem" name="RefreshRepositoryItem" priority="0x301"
                    text="Refres&amp;h"
                    toolTip="Refresh this item." iconFile="menu/Refresh.png"/>
          <Button id="Cmd_ViewInVsNet" name="ViewInVsNet" priority="0x100"
                    text="In &amp;Visual Studio"
                    toolTip="View this file in Visual Studio." iconFile="menu/ViewInVSNet.png"/>
          <Button id="Cmd_ViewInWindows" name="ViewInWindows" priority="0x200"
                  text="In Wi&amp;ndows"
                  toolTip="Have Windows launch the associated application." iconFile="menu/ViewInWindows.png"/>
          <Button id="Cmd_CheckoutFolder" name="CheckoutFolder" priority="0x301"
                    text="Chec&amp;kout Folder..."
                    toolTip="Checkout this folder." iconFile="menu/CheckoutDirectory.png" defaultInvisible="true"/>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_RepositoryExplorerContextMenu">
      <!-- The Repository Explorer Context Menu -->
      <Menu id="Menu_RepositoryExplorerContextMenu" name="RepositoryExplorerContext" priority="0" type="Context">
        <Group id="Grp_RepositoryExplorerContext" priority="0x100">
          <ButtonRef id="Cmd_ViewInVsNet" priority="0x100"/>
          <ButtonRef id="Cmd_ViewInWindows" priority="0x200"/>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_WorkingCopyExplorerToolBar">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="Menu_WorkingCopyExplorerToolBar" name="WorkingCopyExplorer" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_WorkingCopyExplorerToolBar" priority="0x100">
          <Button id="Cmd_AddWorkingCopyExplorerRoot" name="AddWorkingCopyExplorerRoot" priority="0x301"
                    text="A&amp;dd New Root..."
                    toolTip="Add a new root to the Working Copy Explorer." iconFile="menu/AddFolder.png" />
          <Button id="Cmd_RemoveWorkingCopyExplorerRoot" name="RemoveWorkingCopyExplorerRoot" priority="0x301"
        text="&amp;Remove Root"
        toolTip="Remove this root from the Working Copy Explorer." iconFile="menu/RemoveFolder.png"/>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_WorkingCopyExplorerContextMenu">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="Menu_WorkingCopyExplorerContextMenu" name="WorkingCopyExplorerContext" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_WorkingCopyExplorerContext" priority="0x100">
          <ButtonRef id="Cmd_Refresh" priority="0x100" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_PendingChangesToolBar">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="Menu_PendingChangesToolBar" name="PendingChanges" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_PendingChangesToolBar" priority="0x100">
          <ButtonRef id="Cmd_Refresh" priority="0x100" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_PendingChangesContextMenu">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="Menu_PendingChangesContextMenu" name="PendingChangesContext" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_PendingChangesContextMenu" priority="0x100">
          <ButtonRef id="Cmd_Refresh" priority="0x100" />
        </Group>
      </Menu>
    </GroupRef>
  </UI>
</VsGui>
