<?xml version="1.0" encoding="utf-8" ?>
<VsGui version="1.0"  xmlns="http://schemas.studioturtle.net/2007/01/gui/"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://schemas.studioturtle.net/2007/01/gui/ Gui.xsd">
  <Imports>
    <Import include="stdidcmd.h" />
    <Import include="vsshlids.h" />
    <Import include="msobtnid.h" />
    <Import type="Ankh.Ids.AnkhId" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Ankh_" />
    <Import type="Ankh.Ids.AnkhCommand" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Cmd_" />
    <Import type="Ankh.Ids.AnkhCommandGroup" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Grp_" />
    <Import type="Ankh.Ids.AnkhCommandMenu" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Menu_" />
    <Import type="Ankh.Ids.AnkhToolBar" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="ToolBar_" />
  </Imports>
  <Defines>
  </Defines>
  <UI packageId="Ankh_PackageGuid" autoBmpId="Ankh_BmpGuid">
    <MenuRef id="guidSHLMainMenu:IDM_VS_CSCD_OPEN">
      <Group id="Grp_FileFileOpen" priority="0x800">
        <!-- The bottom of the File->Open->* menu-->
        <Button id="Cmd_FileFileOpenFromSubversion" text="Subve&amp;rsion Project..." name="OpenFromSubversion" priority="0x100"
                toolTip="Open a Project or Solution from Subversion" iconFile="PendingChanges/PendingChanges.png" />
      </Group>
    </MenuRef>
    <MenuRef id="guidSHLMainMenu:IDM_VS_CSCD_ADD">
      <Group id="Grp_FileFileAdd" priority="0x800">
        <!-- The bottom of the File->Add->* menu-->
        <Button id="Cmd_FileFileAddFromSubversion" text="Subve&amp;rsion Project..." name="AddFromSubversion" priority="0x100"
                toolTip="Add an existing Project or Solution from Subversion" iconFile="PendingChanges/PendingChanges.png"
                dynamicVisibility="true">
          <!-- Only show Add when not debugging, etc -->
          <Visibility context="UICONTEXT_SolutionExistsAndNotBuildingAndNotDebugging" />
        </Button>
      </Group>
    </MenuRef>
    <MenuRef id="guidSHLMainMenu:IDM_VS_MENU_FILE">
      <!-- Visual Studio file menu -->
      <Group id="Grp_FileMenuScc" priority="0x0800">
        <Menu id="Menu_FileScc" priority="0x0100" name="SvnMenu" text="Subve&amp;rsion">
          <Group id="Grp_FileSccMenuTasks" priority="0x0100">
            <Button id="Cmd_UpdateItem" name="UpdateItem" priority="0x100"
								text="&amp;Update..."
								toolTip="Updates local items with changes from the Subversion repository." iconFile="PendingChanges/Update.png" defaultInvisible="true"/>
            <Button id="Cmd_CommitItem" name="CommitItem" priority="0x200"
								text="&amp;Commit..."
								toolTip="Commit selected items to the Subversion repository." iconFile="menu/Commit.png" defaultInvisible="true"/>
            <Button id="Cmd_RevertItem" name="RevertItem" priority="0x300"
								text="&amp;Revert"
								toolTip="Revert this item to last updated revision." iconFile="menu/Revert.png" defaultInvisible="true"/>
            <Button id="Cmd_Blame" name="Blame" priority="0x400"
								text="&amp;Blame..."
								toolTip="Identify which user wrote which lines." iconFile="menu/Blame.png" defaultInvisible="true"/>
            <Button id="Cmd_DiffLocalItem" name="DiffLocalItem" priority="0x500"
								text="Di&amp;ff..."
								toolTip="Show differences compared to local text base." iconFile="menu/Diff.png" defaultInvisible="true"/>
            <Button id="Cmd_Log" name="Log" priority="0x600"
								text="&amp;Log"
								toolTip="Show the change log for the selected item." iconFile="menu/Log.png" defaultInvisible="true"/>
          </Group>
          <Group id="Grp_FileSccItem" priority="0x0190">
            <ButtonRef id="Cmd_AddItem" priority="0x10" />
          </Group>
          <Group id="Grp_FileSccOpen" priority="0x01A0">
            <Button id="Cmd_FileSccOpenFromSubversion" text="Open &amp;from Subve&amp;rsion..." name="OpenFromSubversion" priority="0x100"
                    toolTip="Open a Project or Solution from Subversion" />
            <Button id="Cmd_FileSccAddFromSubversion" text="A&amp;dd Project from Subversion..." name="AddFromSubversion" priority="0x200"
                    toolTip="Add an existing Project or Solution from Subversion" dynamicVisibility="true">
              <!-- Only show Add when not debugging, etc -->
              <Visibility context="UICONTEXT_SolutionExistsAndNotBuildingAndNotDebugging" />
            </Button>
          </Group>
          <Group id="Grp_FileSccAdd" priority="0x01B0">
            <Button id="Cmd_FileSccAddSolutionToSubversion" name="AddSolutionToRepository" priority="0x100"
							  text="Add &amp;Solution to Subversion..."
							  toolTip="Add this solution to a Subversion repository." iconFile="PendingChanges/AddToSourceControl.ico">
              <Visibility context="UICONTEXT_SolutionExistsAndNotBuildingAndNotDebugging" />
            </Button>
            <Button id="Cmd_FileSccAddProjectToSubversion" name="AddProjectToRepository" priority="0x200"
							  text="Add S&amp;elected Projects to Subversion..."
							  toolTip="Add the selected projects to a Subversion repository." iconFile="PendingChanges/AddToSourceControl.ico">
              <Visibility context="UICONTEXT_SolutionExistsAndNotBuildingAndNotDebugging" />
            </Button>
          </Group>
          <Group id="Grp_FileSccManagement" priority="0x0200">
            <Button id="Cmd_FileSccChangeSourceControl" name="ChangeSourceControl"  priority="0x100"
                    text="Change Source C&amp;ontrol..."
                    toolTip="Change the Source Control bindings of the projects in this solution" iconFile="PendingChanges/ChangeSourceControl.png">
              <Visibility context="UICONTEXT_SolutionExistsAndNotBuildingAndNotDebugging"/>
            </Button>
            <ButtonRef id="Cmd_ShowPendingChanges" priority="0x301" />
            <Button id="Cmd_RunSvnCommand" name="RunSvn" text="RunSvn" priority="0xffff" commandWellOnly="true" allowParams="true" />
          </Group>
        </Menu>
      </Group>
    </MenuRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_VIEW_ORG_WINDOWS">
      <!-- View - The Group containing the Solution Explorer,.. -->
      <Button id="Cmd_ShowPendingChanges" name="ShowPendingChanges" priority="0x301"
								text="Show &amp;Pending Changes"
								toolTip="Show the Pending Changes Window." iconFile="menu/PendingChanges.png" defaultInvisible="true">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="c" />
      </Button>
      <Button id="Cmd_ShowRepositoryExplorer" name="RepositoryExplorer" priority="0x901"
						text="Repositor&amp;y Explorer"
						toolTip="Show the Repository Explorer window." iconFile="menu/ReposExplorer.png">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="r"/>
      </Button>
      <Button id="Cmd_ShowWorkingCopyExplorer" name="WorkingCopyExplorer" priority="0x902"
					text="&amp;Working Copy Explorer"
					toolTip="Show the Working Copy Explorer window." iconFile="menu/WorkExplorer.png" defaultInvisible="true">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="w"/>
      </Button>
    </GroupRef>

    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_SOLUTION_SCC">
      <!-- VS Predefined group for SCC commands on the solution -->
      <ButtonRef id="Cmd_SolutionUpdateHead" priority="0x10" />
      <ButtonRef id="Cmd_CommitItem" priority="0x20" />
      <ButtonRef id="Cmd_DiffLocalItem" priority="0x30" />
      <ButtonRef id="Cmd_RevertItem" priority="0x40" />
      <Menu id="Menu_SolutionExplorerSccForSolution" priority="0x50" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForSolution" priority="0x100">
          <ButtonRef id="Cmd_SwitchItem" priority="0x20" />
          <ButtonRef id="Cmd_Log" priority="0x30" />
          <ButtonRef id="Cmd_Lock" priority="0x40" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x90" />
          <ButtonRef id="Cmd_ResolveConflict" priority="0xA0" />
          <ButtonRef id="Cmd_ResolveConflictExternal" priority="0xB0" />
          <ButtonRef id="Cmd_RevertItem" priority="0xC0" />
          <ButtonRef id="Cmd_RevertToRevision" priority="0xD0" />
          <ButtonRef id="Cmd_Refresh" priority="0xE0" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_PROJECT_SCC">
      <!-- VS Predefined group for SCC commands on projects -->
      <Button id="Cmd_ProjectUpdateHead" priority="0x10" name="ProjectUpdateHead" text="U&amp;pdate to Latest Version"
              iconFile="PendingChanges/Update.png" />
      <ButtonRef id="Cmd_CommitItem" priority="0x20" />
      <ButtonRef id="Cmd_DiffLocalItem" priority="0x30" />
      <ButtonRef id="Cmd_RevertItem" priority="0x40" />
      <Menu id="Menu_SolutionExplorerSccForProject" priority="0x50" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForProject" priority="0x100">
          <Button id="Cmd_AddItem" name="AddItem" priority="0x10"
								text="&amp;Add..."
								toolTip="Add selected items to the working copy." iconFile="menu/Add.png" defaultInvisible="true"/>
          <ButtonRef id="Cmd_SwitchItem" priority="0x20" />
          <ButtonRef id="Cmd_Log" priority="0x30" />
          <ButtonRef id="Cmd_Lock" priority="0x40" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x90" />
          <ButtonRef id="Cmd_ResolveConflict" priority="0xA0" />
          <ButtonRef id="Cmd_ResolveConflictExternal" priority="0xB0" />
          <ButtonRef id="Cmd_RevertItem" priority="0xC0" />
          <ButtonRef id="Cmd_RevertToRevision" priority="0xD0" />
          <ButtonRef id="Cmd_Refresh" priority="0xE0" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_ITEM_SCC">
      <!-- VS Predefined group for SCC commands on project items -->
      <ButtonRef id="Cmd_UpdateItem" priority="0x10" />
      <ButtonRef id="Cmd_CommitItem" priority="0x20" />
      <ButtonRef id="Cmd_DiffLocalItem" priority="0x30" />
      <ButtonRef id="Cmd_RevertItem" priority="0x40" />
      <Menu id="Menu_SolutionExplorerSccForItem" priority="0x50" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForItem" priority="0x100">
          <ButtonRef id="Cmd_AddItem" priority="0x10" />
          <Button id="Cmd_SwitchItem" name="SwitchItem" priority="0x20"
								text="&amp;Switch..."
								toolTip="Switch this item to a different URL." iconFile="menu/Switch.png"/>
          <ButtonRef id="Cmd_Log" priority="0x30" />
          <Button id="Cmd_Lock" name="Lock" priority="0x40"
              text="Loc&amp;k..."
              toolTip="Lock the selected item." iconFile="menu/Lock.png" />
          <Button id="Cmd_Unlock" name="Unlock" priority="0x50"
              text="U&amp;nlock..."
              toolTip="Unlock the selected items." iconFile="menu/Unlock.png"/>
          <Button id="Cmd_Cleanup" name="Cleanup" priority="0x60"
              text="Cl&amp;eanup"
              toolTip="Cleanup the working copy." iconFile="menu/Cleanup.png" defaultInvisible="true"/>
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <Button id="Cmd_CreatePatch" name="CreatePatch" priority="0x80"
              text="Create &amp;Patch..."
              toolTip="Create a patch file of changes." iconFile="menu/CreatePatch.png" defaultInvisible="true"/>
          <Button id="Cmd_DiffExternalLocalItem" name="DiffExternalLocalItem" priority="0x90"
              text="Diff E&amp;xternal"
              toolTip="Diff against local text base using external tool." iconFile="menu/Diff.png" defaultInvisible="true"/>
          <Button id="Cmd_ResolveConflict" name="ResolveConflict" priority="0xA0"
              text="Res&amp;olve Conflict..." defaultInvisible="true"
              toolTip="Resolve conflict between changes." iconFile="menu/ResolveConflict.png"/>
          <Button id="Cmd_ResolveConflictExternal" name="ResolveConflictExternal" priority="0xB0"
              text="Resolve Conflict E&amp;xternal..." defaultInvisible="true"
              toolTip="Resolve conflict between changes using external tool." iconFile="menu/ResolveConflict.png"/>
          <ButtonRef id="Cmd_RevertItem" priority="0xC0" />
          <Button id="Cmd_RevertToRevision" name="RevertToRevision" priority="0xD0"
							text="Re&amp;vert to Revision..."
							toolTip="Revert this item to a specific revision." iconFile="menu/RevertToVersion.png" />
          <Button id="Cmd_Refresh" name="Refresh" priority="0xE0"
								text="Refres&amp;h"
								toolTip="Refresh this view." iconFile="menu/Refresh.png"/>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_RepositoryExplorerToolBar">
      <!-- The Repository Explorer Toolbar -->
      <Menu id="Menu_RepositoryExplorerToolBar" name="RepositoryExplorer" priority="0" type="ToolWindowToolbar">

        <Group id="Grp_RepositoryExplorerToolBar" priority="0x100">
          <Button id="Cmd_Checkout" name="Checkout" priority="0x301"
								text="Chec&amp;kout a Repository..."
								toolTip="Checkout a Subversion repository." iconFile="menu/CheckoutDirectory.png"/>
          <Button id="Cmd_AddRepositoryRoot" name="AddRepositoryRoot" priority="0x301"
							  text="A&amp;dd Repository Url"
							  toolTip="Add a new URL to the Repository Explorer." iconFile="menu/AddUrl.png" />
          <Button id="Cmd_RemoveRepositoryRoot" name="RemoveRepositoryRoot" priority="0x301"
                text="&amp;Remove Repository URL"
                toolTip="Remove a URL from the Repository Explorer." iconFile="menu/RemoveUrl.png"/>
          <Button id="Cmd_CopyReposExplorerUrl" name="CopyReposExplorerUrl" priority="0x301"
							  text="Copy &amp;Url"
							  toolTip="Copy the Url of this item to the clipboard." iconFile="menu/CopyUrlToClipboard.png" />
          <Button id="Cmd_NewDirectory" name="NewDirectory" priority="0x301"
							  text="Ne&amp;w Directory..."
							  toolTip="Create a new directory here." iconFile="menu/MakeDirectory.png" />
          <Button id="Cmd_RefreshRepositoryItem" name="RefreshRepositoryItem" priority="0x301"
							  text="Refres&amp;h"
							  toolTip="Refresh this item." iconFile="menu/Refresh.png"/>
          <Button id="Cmd_ViewInVsNet" name="ViewInVsNet" priority="0x100"
							  text="In &amp;Visual Studio"
							  toolTip="View this file in Visual Studio." iconFile="menu/ViewInVSNet.png"/>
          <Button id="Cmd_ViewInWindows" name="ViewInWindows" priority="0x200"
							text="In Wi&amp;ndows"
							toolTip="Have Windows launch the associated application." iconFile="menu/ViewInWindows.png"/>
          <Button id="Cmd_CheckoutFolder" name="CheckoutFolder" priority="0x301"
							  text="Chec&amp;kout Folder..."
							  toolTip="Checkout this folder." iconFile="menu/CheckoutDirectory.png" defaultInvisible="true"/>
          <Button id="Cmd_SaveToFile" name="SaveToFile" priority="0x301"
              text="Save to &amp;File"
              toolTip="Save the file to disk." iconFile="menu/SaveToFile.png" />
          <Button id="Cmd_CheckoutSolution" name="CheckoutSolution" priority="0x301"
								text="Checkout &amp;Solution..."
								toolTip="Checkout this solution." iconFile="menu/CheckoutSolution.png" defaultInvisible="true"/>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_RepositoryExplorerContextMenu">
      <!-- The Repository Explorer Context Menu -->
      <Menu id="Menu_RepositoryExplorerContextMenu" name="RepositoryExplorerContext" priority="0" type="Context">
        <Group id="Grp_RepositoryExplorerContext" priority="0x100">
          <ButtonRef id="Cmd_ViewInVsNet" priority="0x100"/>
          <ButtonRef id="Cmd_ViewInWindows" priority="0x200"/>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_WorkingCopyExplorerToolBar">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="Menu_WorkingCopyExplorerToolBar" name="WorkingCopyExplorer" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_WorkingCopyExplorerToolBar" priority="0x100">
          <Button id="Cmd_AddWorkingCopyExplorerRoot" name="AddWorkingCopyExplorerRoot" priority="0x301"
						  text="A&amp;dd New Root..."
						  toolTip="Add a new root to the Working Copy Explorer." iconFile="menu/AddFolder.png" />
          <Button id="Cmd_RemoveWorkingCopyExplorerRoot" name="RemoveWorkingCopyExplorerRoot" priority="0x301"
			        text="&amp;Remove Root"
			        toolTip="Remove this root from the Working Copy Explorer." iconFile="menu/RemoveFolder.png"/>
          <Button id="Cmd_Export" name="Export" priority="0x301"
              text="E&amp;xport a Repository or Local Folder..."
              toolTip="Export a Subversion repository or local folder." iconFile="menu/Export.png" defaultInvisible="true"/>
          <Button id="Cmd_ExportFolder" name="ExportFolder" priority="0x301"
              text="E&amp;xport this folder"
              toolTip="Export this folder." iconFile="menu/Export.png" defaultInvisible="true"/>
          <Button id="Cmd_Relocate" name="Relocate" priority="0x301"
              text="Relo&amp;cate..."
              toolTip="Relocate this file." iconFile="menu/Relocate.png" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="Menu_WorkingCopyExplorerContextMenu">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="Menu_WorkingCopyExplorerContextMenu" name="WorkingCopyExplorerContext" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_WorkingCopyExplorerContext" priority="0x100">
          <ButtonRef id="Cmd_Refresh" priority="0x100" />
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="ToolBar_PendingChanges">
      <!-- The Working Copy Explorer Toolbar -->
      <Menu id="ToolBar_PendingChanges" name="PendingChanges" priority="0" type="ToolWindowToolbar">
        <Group id="Grp_PendingChangesSolutionCommands" priority="0x100">
          <Button id="Cmd_PendingChangesSpacer" name="Spacer" text=" " iconAndText="true" priority="0x001"
							toolTip="AnkhSVN" />
          <Menu id="Menu_PendingChangesCommit" name="Commit" text="&amp;Commit" priority="0x100" type="MenuControllerLatched" iconAndText="true">
            <Group id="Menu_PendingChangesCommit" priority="0x100">
              <Button id="Cmd_CommitPendingChanges" name="CommitPendingChanges" priority="0x100"
									text="&amp;Commit Changes"
									toolTip="Commit Pending Changes" fixMenuController="true" iconFile="PendingChanges/Commit.png"/>
              <Button id="Cmd_CommitPendingChangesKeepingLocks" name="CommitPendingChangesKeepingLocks" priority="0x200"
									text="&amp;Commit &amp;Keeping Locks"
									toolTip="Commit pending changes keeping locks after committing" iconFile="PendingChanges/Commit.png"/>
            </Group>
          </Menu>
          <Menu id="Menu_PendingChangesUpdate" priority="0x200" name="Update" text="&amp;Update" type="MenuControllerLatched" iconAndText="true">
            <Group id="Menu_PendingChangesUpdate" priority="0x100">
              <Button id="Cmd_SolutionUpdateHead" name="UpdateHead" priority="0x100"
									text="U&amp;pdate to Latest Version"
									toolTip="Update your solution to the HEAD version"
									fixMenuController="true" iconFile="PendingChanges/Update.png"/>
              <Button id="Cmd_SolutionUpdateSpecific" name="UpdateSpecific" priority="0x200"
									text="Update to a specific &amp;Version"
									toolTip="Update your solution to a specific version" iconFile="PendingChanges/UpdateSpecific.png"/>
            </Group>
          </Menu>
        </Group>
        <Group id="Grp_PendingChangesView" priority="0x200">
          <Menu id="Menu_PendingChangesView" priority="0x100" name="View" type="MenuController">
            <Group id="Menu_PendingChangesView" priority="0x100">
              <Button id="Cmd_PendingChangesViewFlat" name="ViewFlat" priority="0x100"
									text="&amp;Flat"
									toolTip="Flat changes view" iconFile="PendingChanges/FlatView.png"/>
              <Button id="Cmd_PendingChangesViewProject" name="ViewProject" priority="0x200"
									text="&amp;Project"
									toolTip="Project changes view" />
              <Button id="Cmd_PendingChangesViewChangeList" name="ViewChangeList" priority="0x300"
									text="&amp;ChangeList"
									toolTip="ChangeList changes view" />
              <Button id="Cmd_PendingChangesViewFolder" name="ViewFolder" priority="0x400"
									text="&amp;Folders"
									toolTip="Folder changes view" />
            </Group>
          </Menu>
          <Button id="Cmd_PendingChangesViewAll" priority="0x200" name="ViewAll"
							text="View &amp;All" iconFile="PendingChanges/ShowAll.png"/>
          <Button id="Cmd_PendingChangesViewLogMessage" priority="0x300" name="ViewLogMessage"
							text="View &amp;Log message" iconFile="PendingChanges/ShowComment.png"/>
        </Group>
        <Group id="Grp_PendingChangesItems" priority="0x300">
          <Menu id="Menu_ItemConflict" priority="0x100" name="Conflicts" type="MenuControllerLatched" dynamicVisibility="true">
            <Group id="Menu_ItemConflict" priority="0x100">
              <Button id="Cmd_ItemConflictEdit" name="ItemConflictEdit" priority="0x100"
									text="&amp;Edit Conflicts" dynamicVisibility="true" iconFile="PendingChanges/EditConflict.png" fixMenuController="true"/>
              <Button id="Cmd_ItemConflictEditVisualStudio" name="ItemConflictEditVisualStudio" priority="0x200"
									text="Edit in &amp;Visual Studio" dynamicVisibility="true"/>
              <Button id="Cmd_ItemConflictResolvedMerged" name="ItemConflictResolvedMerged" priority="0x300"
									text="Mark as &amp;Resolved" dynamicVisibility="true"/>
              <Button id="Cmd_ItemConflictResolvedMineFull" name="ItemConflictResolvedMineFull" priority="0x400"
									text="Resolve using &amp;My Version" dynamicVisibility="true"/>
              <Button id="Cmd_ItemConflictResolvedTheirsFull" name="ItemConflictResolvedTheirsFull" priority="0x500"
									text="Resolve using &amp;Their Version" dynamicVisibility="true"/>
            </Group>
          </Menu>
          <Menu id="Menu_ItemCompare" priority="0x200" name="Compare" type="MenuControllerLatched" dynamicVisibility="true">
            <Group id="Menu_ItemCompare" priority="0x100">
              <Button id="Cmd_ItemCompareBase" name="CompareBase" priority="0x100"
									text="&amp;Compare to Original Version" fixMenuController="true" iconFile="PendingChanges/Compare.png"/>
              <Button id="Cmd_ItemCompareHead" name="CompareHead" priority="0x200"
									text="Compare to &amp;HEAD Version" />
              <Button id="Cmd_ItemCompareCommitted" name="ComparePrevious" priority="0x300"
									text="Compare to Co&amp;mmitted Version" />
              <Button id="Cmd_ItemComparePrevious" name="ComparePrevious" priority="0x400"
									text="Compare to &amp;Previous Version" />
              <Button id="Cmd_ItemCompareSpecific" name="CompareSpecific" priority="0x500"
									text="Compare to a &amp;Specific Version" />
            </Group>
          </Menu>
          <Menu id="Menu_ItemOpen" priority="0x300" name="Open" type="MenuControllerLatched">
            <Group id="Menu_ItemOpen" priority="0x100">
              <Button id="Cmd_ItemOpenVisualStudio" name="OpenVisualStudio" priority="0x100"
									text="&amp;Open in Visual Studio" fixMenuController="true" iconFile="PendingChanges/Open.png"/>
              <Button id="Cmd_ItemOpenWindows" name="OpenWindows" priority="0x200"
									text="Open with &amp;Default Application" />
              <Button id="Cmd_ItemOpenTextEditor" name="OpenText" priority="0x300"
									text="Open as &amp;Text" />
            </Group>
            <Group id="Grp_PendingChangesOpenEx" priority="0x200">
              <Button id="Cmd_ItemOpenFolder" name="OpenFolder" priority="0x100"
									text="Open Containing &amp;Folder" iconFile="PendingChanges/OpenFolder.png"/>
              <Button id="Cmd_ItemOpenSolutionExplorer" name="FindInSolutionExplorer" priority="0x200"
									text="Select in Solution E&amp;xplorer" iconFile="PendingChanges/SolutionExplorer.png"/>
            </Group>
          </Menu>
          <Menu id="Menu_ItemRevert" priority="0x300" name="Revert" type="MenuControllerLatched">
            <Group id="Menu_ItemRevert" priority="0x100">
              <Button id="Cmd_ItemRevertBase" name="Revert" priority="0x100"
									text="&amp;Revert to BASE Version" iconFile="PendingChanges/Revert.png"/>
              <Button id="Cmd_ItemRevertSpecific" name="RevertSpecific" priority="0x100"
									text="&amp;Revert to a Specific Version" />
            </Group>
          </Menu>
        </Group>
        <Group id="Grp_PendingChangesAdmin" priority="0x400">
          <Menu id="Menu_ItemIgnore" priority="0x100" name="Ignore" type="MenuController">
            <Group id="Menu_ItemIgnore" priority="0x100">
              <Button id="Cmd_ItemIgnoreFile" name="IgnoreFile" priority="0x100"
									text="Ignore &amp;File" iconFile="PendingChanges/Ignore.png"/>
              <Button id="Cmd_ItemIgnoreFileType" name="IgnoreFileType" priority="0x200" textChanges="true"
									text="Ignore File &amp;Type" />
              <Button id="Cmd_ItemIgnoreFilesInFolder" name="IgnoreFilesInFolder" priority="0x300" textChanges="true"
									text="Ignore &amp;All Files in ..." />
