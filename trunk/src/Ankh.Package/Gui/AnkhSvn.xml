<?xml version="1.0" encoding="utf-8" ?>
<VsGui version="1.0"  xmlns="http://schemas.studioturtle.net/2007/01/gui/"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://schemas.studioturtle.net/2007/01/gui/ Gui.xsd">
  <Imports>
    <Import include="stdidcmd.h" />
    <Import include="vsshlids.h" />
    <Import include="msobtnid.h" />
    <Import type="Ankh.Ids.AnkhId" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Ankh_" />
    <Import type="Ankh.Ids.AnkhCommand" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Cmd_" />
    <Import type="Ankh.Ids.AnkhCommandGroup" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Grp_" />
    <Import type="Ankh.Ids.AnkhCommandMenu" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="Menu_" />
    <Import type="Ankh.Ids.AnkhToolBar" from="..\..\Ankh.Ids\bin\$(Configuration)\Ankh.Ids.dll" prefix="ToolBar_" />
  </Imports>
  <Defines>
  </Defines>
  <UI packageId="Ankh_PackageGuid" autoBmpId="Ankh_BmpGuid">
    <MenuRef id="guidSHLMainMenu:IDM_VS_CSCD_OPEN">
      <Group id="Grp_FileFileOpen" priority="0x800">
        <!-- The bottom of the File->Open->* menu-->
        <Button id="Cmd_FileFileOpenFromSubversion" text="Subve&amp;rsion Project..." name="OpenFromSubversion" priority="0x100"
                toolTip="Open a Project or Solution from Subversion" iconFile="PendingChanges/PendingChanges.png" />
      </Group>
    </MenuRef>
    <MenuRef id="guidSHLMainMenu:IDM_VS_CSCD_ADD">
      <Group id="Grp_FileFileAdd" priority="0x800">
        <!-- The bottom of the File->Add->* menu-->
        <Button id="Cmd_FileFileAddFromSubversion" text="Subve&amp;rsion Project..." name="AddFromSubversion" priority="0x100"
                toolTip="Add an existing Project or Solution from Subversion" iconFile="PendingChanges/PendingChanges.png"
                dynamicVisibility="true">
          <!-- Only show Add when not debugging, etc -->
          <Visibility context="UICONTEXT_SolutionExistsAndNotBuildingAndNotDebugging" />
        </Button>
      </Group>
    </MenuRef>
    <MenuRef id="guidSHLMainMenu:IDM_VS_MENU_FILE">
      <!-- Visual Studio file menu -->
      <Group id="Grp_FileMenuScc" priority="0x0800">
        <Menu id="Menu_FileScc" priority="0x0100" name="SvnMenu" text="Subve&amp;rsion">
          <Group id="Grp_FileSccMenuTasks" priority="0x0100">
            <Button id="Cmd_MigrateSettings" text="Migrate AnkhSVN Settings from future versions" name="AnkhSVN.MigrateSettings" priority="0" commandWellOnly="true" allowParams="true" postExec="true"/>
            <Button id="Cmd_UpdateItem" name="UpdateItem" priority="0x100"
								text="&amp;Update..."
								toolTip="Updates local items with changes from the Subversion repository." iconFile="PendingChanges/Update.png" defaultInvisible="true"/>
            <Button id="Cmd_CommitItem" name="CommitItem" priority="0x200"
								text="&amp;Commit..."
								toolTip="Commit selected items to the Subversion repository." iconFile="PendingChanges/Commit.png" defaultInvisible="true"/>
            <Button id="Cmd_RevertItem" name="RevertItem" priority="0x300"
								text="&amp;Revert"
								toolTip="Revert this item to last updated revision." iconFile="menu/Revert.png" defaultInvisible="true"/>
            <Button id="Cmd_Blame" name="Blame" priority="0x400"
								text="&amp;Blame..."
								toolTip="Identify which user wrote which lines." iconFile="menu/Blame.png" defaultInvisible="true"/>
            <Button id="Cmd_DiffLocalItem" name="DiffLocalItem" priority="0x500"
								text="Di&amp;ff..."
								toolTip="Show differences compared to local text base." iconFile="PendingChanges/Compare.png" defaultInvisible="true"/>
            <ButtonRef id="Cmd_Log" priority="0x600" />
          </Group>
          <Group id="Grp_FileSccItem" priority="0x0190">
            <ButtonRef id="Cmd_AddItem" priority="0x10" />
          </Group>
          <Group id="Grp_FileSccOpen" priority="0x01A0">
            <Button id="Cmd_FileSccOpenFromSubversion" text="Open &amp;from Subve&amp;rsion..." name="OpenFromSubversion" priority="0x100"
                    toolTip="Open a Project or Solution from Subversion" dynamicVisibility="true">
              <Visibility context="Ankh_SccProviderGuid" />
            </Button>
            <Button id="Cmd_FileSccAddFromSubversion" text="A&amp;dd Project from Subversion..." name="AddFromSubversion" priority="0x200"
                    toolTip="Add an existing Project or Solution from Subversion" dynamicVisibility="true">
              <!-- Only show Add when not debugging, etc -->
              <Visibility context="Ankh_SccProviderGuid" />
            </Button>
          </Group>
          <Group id="Grp_FileSccAdd" priority="0x01B0">
            <Button id="Cmd_FileSccAddSolutionToSubversion" name="AddSolutionToRepository" priority="0x100"
							  text="Add &amp;Solution to Subversion..."
							  toolTip="Add this solution to a Subversion repository." iconFile="PendingChanges/AddToSourceControl.ico">
              <Visibility context="Ankh_SccProviderGuid" />
            </Button>
            <Button id="Cmd_FileSccAddProjectToSubversion" name="AddProjectToRepository" priority="0x200"
							  text="Add S&amp;elected Projects to Subversion..."
							  toolTip="Add the selected projects to a Subversion repository." iconFile="PendingChanges/AddToSourceControl.ico">
              <Visibility context="Ankh_SccProviderGuid" />
            </Button>
          </Group>
          <Group id="Grp_FileSccManagement" priority="0x0200">
            <Button id="Cmd_FileSccChangeSourceControl" name="ChangeSourceControl"  priority="0x100"
                    text="Change Source C&amp;ontrol..."
                    toolTip="Change the Source Control bindings of the projects in this solution" iconFile="PendingChanges/ChangeSourceControl.png">
              <Visibility context="Ankh_SccProviderGuid"/>
            </Button>
            <ButtonRef id="Cmd_ShowPendingChanges" priority="0x301" />
          </Group>
        </Menu>
      </Group>
    </MenuRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_VIEW_ORG_WINDOWS">
      <!-- View - The Group containing the Solution Explorer,.. -->
      <Button id="Cmd_ShowPendingChanges" name="ShowPendingChanges" priority="0x301"
								text="Show &amp;Pending Changes"
								toolTip="Show the Pending Changes Window." iconFile="menu/PendingChanges.png" defaultInvisible="true">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="c" />
      </Button>
      <Button id="Cmd_ShowRepositoryExplorer" name="RepositoryExplorer" priority="0x901"
						text="Repositor&amp;y Explorer"
						toolTip="Show the Repository Explorer window." iconFile="menu/ReposExplorer.png">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="r"/>
      </Button>
      <Button id="Cmd_ShowWorkingCopyExplorer" name="WorkingCopyExplorer" priority="0x902"
					text="&amp;Working Copy Explorer"
					toolTip="Show the Working Copy Explorer window." iconFile="menu/WorkExplorer.png" defaultInvisible="true">
        <KeyBinding editor="guidVSStd97" mod1="ctrl" key1="k" key2="w"/>
      </Button>
    </GroupRef>

    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_SOLUTION_SCC">
      <!-- VS Predefined group for SCC commands on the solution -->
      <ButtonRef id="Cmd_FileSccAddSolutionToSubversion" priority="0x1100" />
      <ButtonRef id="Cmd_FileSccAddProjectToSubversion" priority="0x1200" />
      <ButtonRef id="Cmd_SolutionUpdateHead" priority="0x1500" />
      <Button id="Cmd_SolutionCommit" priority="0x1600" name="SolutionCommit" defaultInvisible="true"
              text="Comm&amp;it Solution Changes" iconFile="PendingChanges/Commit.png" />
      <ButtonRef id="Cmd_ItemShowChanges" priority="0x1700" />
      <Button id="Cmd_SolutionHistory" priority="0x1800" name="SolutionHistory" defaultInvisible="true"
              text="View Solution &amp;History" iconFile="PendingChanges/ViewHistory.png" />
      <ButtonRef id="Cmd_RevertItem" priority="0x1900" />
      <Menu id="Menu_SolutionExplorerSccForSolution" priority="0x1f00" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForSolution" priority="0x100">
          <ButtonRef id="Cmd_DiffLocalItem" priority="0x10" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x20" />
          <ButtonRef id="Cmd_SolutionSwitchDialog" priority="0x30" />
          <ButtonRef id="Cmd_Lock" priority="0x40" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />
          <Button id="Cmd_RevertSolutionToRevision" priority="0xD0"
                  text="Revert solution to revision..."
                  toolTip="Revert this item to a specific revision."
                  iconFile="menu/RevertToVersion.png"
                  defaultInvisible="true"/>
          <Button id="Cmd_SolutionMerge" priority="0xE0" name="MergeSolution"
              text="&amp;Merge Solution..." iconFile="menu/Merge.png" defaultInvisible="true"/>          
          <ButtonRef id="Cmd_Refresh" priority="0xF0" />
        </Group>
        <Group id="Grp_SccExplore" priority="0xF000">
          <Button id="Cmd_ItemOpenFolderInWorkingCopyExplorer" name="OpenInWorkingCopyExplorer" priority="0x200"
                      text="Open Folder in &amp;Working Copy Explorer" defaultInvisible="true">
            <Visibility context="Ankh_SccProviderGuid"/>
          </Button>
          <Button id="Cmd_ItemOpenFolderInRepositoryExplorer" name="OpenInRepositoryExplorer" priority="0x300"
                      text="Open Folder in &amp;Repository Explorer" defaultInvisible="true">
            <Visibility context="Ankh_SccProviderGuid"/>
          </Button>
        </Group>
      </Menu>
    </GroupRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_PROJECT_SCC">
      <!-- VS Predefined group for SCC commands on projects -->
      <ButtonRef id="Cmd_FileSccAddSolutionToSubversion" priority="0x1100" />
      <ButtonRef id="Cmd_FileSccAddProjectToSubversion" priority="0x1200" />
      <Button id="Cmd_ProjectUpdateHead" priority="0x1500" name="ProjectUpdateHead" text="U&amp;pdate to Latest Version"
              iconFile="PendingChanges/Update.png" defaultInvisible="true"/>
      <Button id="Cmd_ProjectCommit" priority="0x1600" name="ProjectCommit"
              text="Comm&amp;it Project Changes" iconFile="PendingChanges/Commit.png" defaultInvisible="true"/>
      <ButtonRef id="Cmd_ItemShowChanges" priority="0x1700" />
