          <ButtonRef id="Cmd_DiffLocalItem" priority="0x20" />
          <ButtonRef id="Cmd_DiffExternalLocalItem" priority="0x30" />
          <Button id="Cmd_SwitchProject" name="SwitchProject" priority="0x40" defaultInvisible="true"
								text="&amp;Switch Project..." allowParams="true"
								toolTip="Switch this project to a different Url." iconFile="PendingChanges/Switch.png"/>
          <ButtonRef id="Cmd_Lock" priority="0x45" />
          <ButtonRef id="Cmd_Unlock" priority="0x50" />
          <ButtonRef id="Cmd_Cleanup" priority="0x60" />
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <ButtonRef id="Cmd_CreatePatch" priority="0x80" />

          <Button id="Cmd_RevertProjectToRevision" priority="0xD0" 
                  text="Revert project to revision..." 
                  toolTip="Revert this item to a specific revision." 
                  iconFile="menu/RevertToVersion.png"
                  defaultInvisible="true"/>
          <Button id="Cmd_ProjectMerge" priority="0xE0" name="MergeProject"
              text="&amp;Merge Project..." iconFile="menu/Merge.png" defaultInvisible="true"/>
          <ButtonRef id="Cmd_Refresh" priority="0xF0" />
        </Group>
        <GroupRef id="Grp_SccExplore" priority="0xF000" />
      </Menu>
    </GroupRef>
    <GroupRef id="guidSHLMainMenu:IDG_VS_CTXT_ITEM_SCC">
      <!-- VS Predefined group for SCC commands on project items -->
      <ButtonRef id="Cmd_UpdateItem" priority="0x1500" />
      <ButtonRef id="Cmd_CommitItem" priority="0x1600" />
      <Button id="Cmd_ItemShowChanges" name="SccCompare" priority="0x1700"
