<?xml version="1.0" ?>
<xs:schema id="Config" targetNamespace="http://ankhsvn.com/Config.xsd" xmlns:mstns="http://ankhsvn.com/Config.xsd"
    xmlns="http://ankhsvn.com/Config.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
    attributeFormDefault="qualified" elementFormDefault="qualified">
    <xs:element name="Config" msdata:IsDataSet="true" msdata:Locale="nb-NO" msdata:EnforceConstraints="False">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="RepositoryExplorer">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="MruUrls" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Url" nillable="true" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:simpleContent msdata:ColumnName="Url_Text" msdata:Ordinal="0">
                                                    <xs:extension base="xs:string"></xs:extension>
                                                </xs:simpleContent>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="MergeExePath" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element name="DiffExePath" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element name="LogMessageTemplate" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element name="AutoAddNewFiles" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                <xs:element name="AutoReuseComment" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                <xs:element name="ChooseDiffMergeManual" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                <xs:element name="DisableSolutionReload" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                <xs:element name="Subversion">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="ConfigDir" type="xs:string" minOccurs="0" maxOccurs="1" />
                            <xs:element name="SvnExePath" type="xs:string" minOccurs="0" maxOccurs="1" />
                            <xs:element name="AdminDirectoryName" type="xs:string" minOccurs="0" maxOccurs="1" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
