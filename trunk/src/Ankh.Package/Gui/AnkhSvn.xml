      <ButtonRef id="Cmd_RevertItem" priority="0x1A00" />
      <Menu id="Menu_SolutionExplorerSccForProject" priority="0x1B00" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForProject" priority="0x100">
          <Button id="Cmd_AddItem" name="AddItem" priority="0x10"
								text="&amp;Add..."
