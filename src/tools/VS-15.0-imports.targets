    <Reference Include="Microsoft.VisualStudio.Imaging.Interop.14.0.DesignTime" Condition="'$(UseVsSdkVersion)' != '8.0' and '$(UseVsSdkVersion)' != '9.0' and '$(UseVsSdkVersion)' &gt;= '14.0'">
