          <Button id="Cmd_ViewInVsNet" name="ViewInVsNet" priority="0x700"
							  text="In &amp;Visual Studio"
							  toolTip="View this file in Visual Studio." iconFile="menu/ViewInVSNet.png"/>
          <Button id="Cmd_ViewInWindows" name="ViewInWindows" priority="0x800"
							text="In Wi&amp;ndows"
							toolTip="Have Windows launch the associated application." iconFile="menu/ViewInWindows.png"/>
          <Button id="Cmd_CheckoutFolder" name="CheckoutFolder" priority="0x900"
							  text="Chec&amp;kout Folder..."
