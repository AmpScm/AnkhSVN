<?xml version='1.0'?>
<?xml-stylesheet type='text/xsl' href='..\UseCaseXSL1.xsl'?>
<UseCase>
    <Preface>
        <Name>Show SVN feedback</Name>
        <ID>ShowSvnFeedback</ID>
        <Actors></Actors>
        <Summary>Feedback from an SVN operation is displayed inside VS.NET</Summary>
    </Preface>
    <MainFlow>
        <Preconditions>
            <Precondition>The user has initiated an operation that needs to provide feedback</Precondition>
        </Preconditions>
        <FlowElements>
            <FlowElement>
                <Step>A new Output window is created in VS.NET</Step>
            </FlowElement>
            <FlowElement>
                <Step>The window receives focus</Step>
            </FlowElement>
            <FlowElement>
                <Step>Output messages are written to the window</Step>
            </FlowElement>
        </FlowElements>
        <Postconditions>
            <Postcondition>The developer is enlightened</Postcondition>
        </Postconditions>
    </MainFlow>
</UseCase>
