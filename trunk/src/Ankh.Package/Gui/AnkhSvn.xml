        </Group>
      </Menu>
      <ButtonRef id="Cmd_RevertItem" priority="0x1B00" />
      <Menu id="Menu_SolutionExplorerSccForItem" priority="0x1D00" name="Subversion" text="&amp;Subversion">
        <Group id="Grp_SolutionExplorerSccForItem" priority="0x100">
          <ButtonRef id="Cmd_AddItem" priority="0x10" />
          <ButtonRef id="Cmd_DiffLocalItem" priority="0x20" />
          <Button id="Cmd_DiffExternalLocalItem" name="DiffExternalLocalItem" priority="0x30"
              text="Diff E&amp;xternal"
              toolTip="Diff against local text base using external tool." iconFile="PendingChanges/Compare.png" defaultInvisible="true"/>
          <Button id="Cmd_SwitchItem" name="SwitchItem" priority="0x40" defaultInvisible="true"
								text="&amp;Switch..." allowParams="true"
								toolTip="Switch this item to a different Url." iconFile="PendingChanges/Switch.png"/>
          <Button id="Cmd_Lock" name="Lock" priority="0x45" defaultInvisible="true"
              text="Loc&amp;k..."
              toolTip="Lock the selected item." iconFile="menu/Lock.png" />
          <Button id="Cmd_Unlock" name="Unlock" priority="0x50" defaultInvisible="true"
              text="U&amp;nlock..."
              toolTip="Unlock the selected items." iconFile="menu/Unlock.png"/>
          <Button id="Cmd_Cleanup" name="Cleanup" priority="0x60" defaultInvisible="true"
              text="Cl&amp;eanup"
              toolTip="Cleanup the working copy." iconFile="menu/Cleanup.png"/>
          <ButtonRef id="Cmd_Blame" priority="0x70" />
          <Button id="Cmd_CreatePatch" name="CreatePatch" priority="0x80" defaultInvisible="true"
              text="Create &amp;Patch..."
              toolTip="Create a patch file of changes." iconFile="menu/CreatePatch.png"/>
					<Button id="Cmd_ItemEditProperties" priority="0x90" defaultDisabled="true"
									text="Prop&amp;erties..."
									toolTip="View/Edit Subversion Properties" 
                  iconFile="menu/Properties.png" defaultInvisible="true"/>
          <Button id="Cmd_RevertToRevision" name="RevertToRevision" priority="0xD0" defaultInvisible="true"
