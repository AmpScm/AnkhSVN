$projectdir = $args[0]
$providerdll = $args[1]
$register = combine-path $MSHHOME "Export-Command.exe"
&$register $providerdll

$display = combine-path $projectdir "*.display.xml"
$types = combine-path $projectdir "*.types.xml"

write-error $() -errortext "copy-item $display $MSHHOME"

copy-item $display $MSHHOME
copy-item $types $MSHHOME
